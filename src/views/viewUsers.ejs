<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>View USers</title>
  </head>
  <body>
    <table>
      <th>ID</th>
      <th>Email</th>
      <th>City</th>
      <%data.forEach(function(user) {%>
      <tr>
        <td><%=user.id%></td>
        <td><%=user.email%></td>
        <td><%=user.city%></td>
        <td>
        <button>Edit</button>
        <button>Delete</button>
        </td>
      </tr>
      
      <%});%>
    </table>
  </body>
</html>
